NODE_ENV=development
PORT=5000
HOST=127.0.0.1
DATABASE_URL=postgresql://username:password@localhost:5432/dbname
REDIS_URL=redis://127.0.0.1:6379
SESSION_SECRET=your-session-secret-here

# Payment Gateway Test Keys (Replace with your own test keys)
# For Stripe: https://dashboard.stripe.com/test/apikeys
STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here

# For Adyen: https://docs.adyen.com/development-resources/test-api-credentials/
ADYEN_API_KEY=test_your_api_key_here
ADYEN_MERCHANT_ACCOUNT=YourMerchantAccountTest

# For Square: https://developer.squareup.com/apps
SQUARE_ACCESS_TOKEN=sandbox_access_token_here
SQUARE_ENVIRONMENT=sandbox

# For Braintree: https://sandbox.braintreegateway.com/
BRAINTREE_MERCHANT_ID=your_merchant_id
BRAINTREE_PUBLIC_KEY=your_public_key
BRAINTREE_PRIVATE_KEY=your_private_key

# Set to 'mock' or leave empty for any gateway to use mock mode
# Example: STRIPE_SECRET_KEY=mock
